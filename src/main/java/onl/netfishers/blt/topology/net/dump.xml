<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	package-name="onl.netfishers.blt.topology.net">
	<java-types>
		<java-type name="Network">
			<java-attributes>
				<xml-elements java-attribute="routers">
					<xml-element-wrapper name="routers" />
					<xml-element name="router" />
				</xml-elements>
			</java-attributes>
		</java-type>
		<java-type name="Router">
			<java-attributes>
				<xml-transient java-attribute="ipv4IgpRoutes" />
				<xml-transient java-attribute="needTeRefresh" />
				<xml-transient java-attribute="lost" />
				<xml-transient java-attribute="routerInterfaces" />
			</java-attributes>
		</java-type>
	</java-types>
</xml-bindings>